{
  "name": "iconttv-discord-hook",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:build": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "dev": "concurrently \"yarn dev:build\" \"cross-env NODE_ENV=development nodemon ./dist/index.js\"",
    "build": "tsc && tsc-alias",
    "start": "node ./dist/index.js",
    "eslint": "eslint src/ --ext .ts",
    "lint": "yarn eslint",
    "lint:format": "yarn lint --fix"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.5.9",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^1.5.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
