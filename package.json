{
  "name": "iconttv-discord-hook",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "cli": "cross-env NODE_ENV=prod tsx ./src/cli.ts",
    "dev": "cross-env NODE_ENV=dev nodemon --exec tsx ./src/index.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=prod node ./dist/index.js",
    "archive-dev": "cross-env NODE_ENV=dev nodemon --exec tsx ./src/archive.ts",
    "archive-start": "cross-env NODE_ENV=prod node ./dist/archive.js",
    "cmd-dev": "cross-env NODE_ENV=dev tsx ./src/deploy-commands.ts",
    "cmd-prod": "cross-env NODE_ENV=prod tsx ./src/deploy-commands.ts",
    "eslint": "eslint src/ --ext .ts",
    "lint": "yarn eslint",
    "lint:format": "yarn lint --fix"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.0.0",
    "@types/safe-json-stringify": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "nodemon": "^3.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.3.1",
    "axios": "^1.13.5",
    "better-sqlite3": "^12.4.1",
    "commander": "^14.0.1",
    "discord.js": "14.25.1",
    "dotenv": "^16.3.1",
    "es-toolkit": "^1.32.0",
    "jszip": "^3.10.1",
    "kafkajs": "^2.2.4",
    "moment": "^2.29.4",
    "mongoose": "^9.0.0",
    "node-html-markdown": "^1.3.0",
    "openai": "^6.4.0",
    "safe-json-stringify": "^1.2.0",
    "sharp": "^0.34.3",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "resolutions": {
    "brace-expansion@^1.1.7": "1.1.12",
    "brace-expansion@^2.0.1": "2.0.2",
    "glob@^11.0.0": "11.1.0",
    "js-yaml@^4.1.0": "4.1.1",
    "lodash@^4.17.21": "4.17.23",
    "undici@6.21.3": "6.23.0"
  },
  "packageManager": "yarn@4.12.0"
}
